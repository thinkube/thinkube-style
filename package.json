{
  "name": "thinkube-style",
  "version": "0.1.0",
  "description": "React component library and style guide for Thinkube applications, built with Vite and shadcn/ui",
  "author": "Alejandro Martínez Corriá",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkube/thinkube-style.git"
  },
  "bugs": {
    "url": "https://github.com/thinkube/thinkube-style/issues"
  },
  "homepage": "https://github.com/thinkube/thinkube-style",
  "keywords": [
    "thinkube",
    "react",
    "components",
    "shadcn",
    "ui",
    "style-guide"
  ],
  "main": "./dist/components/index.js",
  "types": "./dist/components/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "tailwind.config.ts",
    "postcss.config.mjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "tsc -p tsconfig.lib.json && tsc-alias -p tsconfig.lib.json",
    "prepare": "npm run build:lib",
    "prepublishOnly": "npm run build:lib",
    "preview": "vite preview",
    "lint": "eslint"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^9.0.0",
    "@fontsource/noto-sans-mono": "^5.2.10",
    "@fontsource/poppins": "^5.2.7",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9",
    "tailwindcss": "^4.1.16",
    "tsc-alias": "^1.8.16",
    "tw-animate-css": "^1.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.0"
  }
}
